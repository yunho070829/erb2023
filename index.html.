<body>
<h1>sum 구하기</h1>
<script>
sum = 0
//data = [1, 2, 3, 4, 5]
data = prompt('input Numbers")
data = data.split(' ')
data = data.map(Number)
for (i=0 ; i<=data.length ; i++){
    sum = sum + data[i]
 //document.write("i is" + i + " ,data is" data[i] +<br>)
}
document.write("sum is" +  sum +<br>)
</script>
</body>
